  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "config": {
            "name": "cra_electron_forge_demo"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "build": {
    "files": [
      "public/**/*",
      "build/**/*"
    ],
    "extraFiles" : [
      "pythonServer/**/*"
    ],
    "extends": null,
    "appId": "com.muswada.und",
    "artifactName": "${productName}-${version}-${arch}.${ext}",
    "squirrelWindows": {
      "iconUrl": "public/fav.ico"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    }
